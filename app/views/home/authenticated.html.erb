    // Redirect external links
  
  $("a[rel='external']").click(function(){
    this.target = "_blank";
  });   
  
  
  // Modernizr SVG backup
  
  if(!Modernizr.svg) {
      $('img[src*="svg"]').attr('src', function() {
          return $(this).attr('src').replace('.svg', '.png');
      });
